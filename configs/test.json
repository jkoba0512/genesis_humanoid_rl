{
    "env": {
        "episode_length": 100,
        "simulation_fps": 100,
        "control_freq": 20,
        "target_velocity": 1.0,
        "n_envs": 1
    },
    "algorithm": {
        "learning_rate": 3e-4,
        "n_steps": 512,
        "batch_size": 64,
        "n_epochs": 5,
        "gamma": 0.99,
        "gae_lambda": 0.95,
        "clip_range": 0.2,
        "ent_coef": 0.01,
        "vf_coef": 0.5,
        "max_grad_norm": 0.5,
        "policy_kwargs": {
            "net_arch": [128, 128],
            "activation_fn": "tanh"
        }
    },
    "training": {
        "total_timesteps": 5000,
        "save_freq": 2500,
        "eval_freq": 1000,
        "eval_episodes": 2,
        "log_interval": 1,
        "experiment_name": "test_sb3",
        "log_dir": "./logs/test_sb3",
        "model_dir": "./models/test_sb3"
    }
}